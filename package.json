{
  "name": "curd-pro",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@10.0.0",
  "type": "module",
  "scripts": {
    "changeset": "changeset",
    "dev:playground": "pnpm --filter @fcurd/playground dev",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "version-packages": "changeset version",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "pnpm -r build && changeset publish"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.2.0",
    "@changesets/cli": "^2.29.7",
    "@fcurd/core": "npm:@fcurd/core@^0.1.0",
    "@fcurd/naive-ui": "npm:@fcurd/naive-ui@^0.1.0",
    "@types/node": "^22.19.7",
    "@vitejs/plugin-vue": "^6.0.3",
    "eslint": "^9.39.2",
    "mermaid": "^11.12.3",
    "naive-ui": "^2.43.2",
    "rimraf": "^6.0.1",
    "tsup": "^8.5.0",
    "typescript": "^5.9.2",
    "vite": "^7.3.1",
    "vitepress": "^1.6.4",
    "vitepress-demo-plugin": "^1.5.1",
    "vitepress-plugin-mermaid": "^2.0.17",
    "vue": "^3.5.27",
    "vue-router": "^4.6.0",
    "vue-tsc": "^3.2.3"
  }
