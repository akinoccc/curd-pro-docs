{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "noEmit": true,
    "types": ["vue"],
    "paths": {
      "@uozi/vito-core": ["./packages/core/src/index.ts"],
      "@uozi/vito-naive-ui": ["./packages/naive-ui/src/index.ts"]
    }
  }
}
